{
    "env": {
        "browser": true,
        "es6": true,
        "worker": true,
        "shared-node-browser": true,
        "serviceworker": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": [
            "error",
            {
            "allow": ["warn", "error"]
        }]
    }

}